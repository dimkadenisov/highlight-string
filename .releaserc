{
  "branches": [
    "master"
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES",
            "BREAKING",
            "BREAKING_CHANGE",
            "BREAKING_CHANGES",
            "BREAKING-CHANGE",
            "BREAKING-CHANGES"
          ]
        },
        "releaseRules": [
          {
            "release": "patch",
            "type": "ci"
          },
          {
            "release": "patch",
            "type": "docs"
          },
          {
            "release": "patch",
            "type": "refactor"
          },
          {
            "release": "patch",
            "type": "style"
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES",
            "BREAKING",
            "BREAKING_CHANGE",
            "BREAKING_CHANGES",
            "BREAKING-CHANGE",
            "BREAKING-CHANGES"
          ]
        },
        "writerOpts": {
          "commitsSort": [
            "subject",
            "scope"
          ]
        }
      }
    ],
    "@semantic-release/npm",
    "@semantic-release/github"
  ]
}
